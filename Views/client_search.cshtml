@{
    Layout="Shared/_Layout.cshtml";
}

<div class="well">
  <h2>Search result:</h2>
  @if(@Model.Count== 0)
  {
    <h5> There is no match for your search</h5>
  }
  else
  {
    <ol>
    @foreach (var client in @Model)
    {
      <li><a href="/clients/@client.GetId()"> @client.GetName()</a></li>
    }
    </ol>
  }
</div>

<a href="/">Home Page</a>
<a href="/stylists">See all stylists</a>
<a href="/clients">See all clients</a>
